{
  "name": "@qbane/sveltekit-template",
  "version": "0.0.4",
  "description": "My opinionated SvelteKit template in pure JavaScript.",
  "author": "qbane",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/kit": "^2.21.4",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "svelte": "^5.34.1",
    "svelte-check": "^4.2.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
  },
  "engines": {
    "node": ">=18.0.0 <23.0.0"
  }
}
