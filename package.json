{
  "name": "@qbane/sveltekit-template",
  "version": "0.0.2",
  "description": "My opinionated SvelteKit template in pure JavaScript.",
  "author": "qbane",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^5.2.10",
    "@sveltejs/kit": "^2.12.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.2",
    "svelte": "^5.14.0",
    "svelte-check": "^4.1.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vitest": "^3.0.0-beta.1"
  },
  "engines": {
    "node": ">=18.0.0 <23.0.0"
  },
  "overrides": {
    "vitest": {
      "vite": "^6.0.0"
    }
  }
}
