{
  "name": "@qbane/sveltekit-template",
  "version": "0.0.1",
  "description": "My opinionated SvelteKit template in pure JavaScript.",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "vitest"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^5.1.1",
    "@sveltejs/kit": "^2.5.16",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.0",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0 <21.0.0"
  },
  "type": "module"
}
