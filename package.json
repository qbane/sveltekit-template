{
  "name": "@qbane/sveltekit-template",
  "version": "0.0.1",
  "description": "My opinionated SvelteKit template in pure JavaScript.",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "vitest"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/kit": "^1.27.3",
    "svelte": "^4.2.2",
    "svelte-check": "^3.5.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "type": "module"
}
